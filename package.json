{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "Sean Keever's personal website",
  "version": "0.1.0",
  "author": "Sean Keever",
  "dependencies": {
    "@reach/router": "^1.3.3",
    "autoprefixer": "^9.8.0",
    "gatsby": "^2.21.37",
    "gatsby-image": "^2.4.5",
    "gatsby-plugin-manifest": "^2.4.5",
    "gatsby-plugin-minify-html": "^1.0.1",
    "gatsby-plugin-offline": "^3.2.3",
    "gatsby-plugin-postcss": "^2.3.2",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.6.9",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.3.8",
    "gatsby-remark-vscode": "^2.1.1",
    "gatsby-source-filesystem": "^2.3.8",
    "gatsby-transformer-remark": "^2.8.13",
    "gatsby-transformer-sharp": "^2.5.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "react-icons": "^3.10.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.10",
    "gh-pages": "^3.0.0",
    "husky": ">=4",
    "lint-staged": ">=10",
    "prettier": "2.0.5",
    "tailwindcss": "^1.4.6"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "dev-m": "./scripts/runDevServer.sh",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yml}\" && npx eslint --fix \"*.{js,jsx,ts,tsx}\"",
    "start": "npm run develop",
    "deploy": "gatsby build && gh-pages -d public -b master",
    "reinstall": "rm package-lock.json; rm -rf node_modules && npm install",
    "serve": "gatsby serve",
    "clean": "gatsby clean && rm -rf .cache",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
